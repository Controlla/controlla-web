{
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore . && stylelint --fix \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/eslint-module": "^4.0.2",
    "@nuxtjs/i18n": "^8.0.0-beta.11",
    "@nuxtjs/tailwindcss": "^6.4.1",
    "@vite-pwa/nuxt": "^0.0.7",
    "@vueuse/core": "^9.13.0",
    "@vueuse/motion": "2.0.0-beta.25",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "locomotive-scroll": "^4.1.4",
    "nuxt": "^3.2.3",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-loader": "^7.0.2",
    "prettier": "^2.8.7",
    "sass": "^1.59.2",
    "sass-loader": "^13.2.0",
    "stylelint": "^15.5.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "swiper": "^8.4.7",
    "typescript": "^4.9.5",
    "vitest": "^0.30.1",
    "vue-awesome-swiper": "^5.0.1"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.7",
    "pinia": "^2.0.33",
    "tailwindcss-animation-delay": "^1.0.7"
  }
}
